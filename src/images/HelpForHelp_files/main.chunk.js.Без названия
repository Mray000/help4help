  switch (action.type) {
    case FOLLOW:
      return { ...state,
        users: Object(_utils_ArrayTreatment__WEBPACK_IMPORTED_MODULE_1__["ArrayMaping"])(state.users, "id", action, {
          followed: true
        })
      };

    case UN_FOLLOW:
      return { ...state,
        users: Object(_utils_ArrayTreatment__WEBPACK_IMPORTED_MODULE_1__["ArrayMaping"])(state.users, "id", action, {
          followed: false
        })
      };

    case PAGE_CHOISE:
      return { ...state,
        currentPage: action.pageNumber
      };

    case TOTAL_USERS_COUNT:
      return { ...state,
        totalUsersCount: action.totalUsersCount
      };

    case TOGGLE_IS_FETCHING:
      return { ...state,
        isFetching: action.isFetching
      };

    case SET_USERS:
      return { ...state,
        users: action.users
      };

    case TOGGLE_IS_FOLLOWING:
      return { ...state,
        isFollowing: action.expected ? [...state.isFollowing, action.id] : Object(_utils_ArrayTreatment__WEBPACK_IMPORTED_MODULE_1__["ArrayFilter"])(state.isFollowing, null, action.id)
      };

    default:
      return state;
  }
