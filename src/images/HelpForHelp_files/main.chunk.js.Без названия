/***/ "./src/utils/ArrayTreatment.js":
/*!*************************************!*\
  !*** ./src/utils/ArrayTreatment.js ***!
  \*************************************/
/*! exports provided: ArrayMaping, ArrayFilter */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "ArrayMaping", function() { return ArrayMaping; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "ArrayFilter", function() { return ArrayFilter; });
const ArrayMaping = (massive, comparisonEl, action, difference) => {
  return massive.map(el => {
    if (el[comparisonEl] === action[comparisonEl]) {
      return { ...el,
        ...difference
      };
    }

    return el;
  });
};
const ArrayFilter = (massive, comparisonEl, action) => {
  return massive.filter(el => el[comparisonEl] !== action[comparisonEl]);
};

